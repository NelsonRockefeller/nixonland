let e = campaignTrail_temp;
e.global_parameter_json[0].fields.question_count = 25

function ctsAchievement(achievement, difficultyChecker = true){
	if ((difficultyChecker && campaignTrail_temp.difficulty_level_multiplier<=1)||!difficultyChecker)
		if(campaignTrail_temp.CTS){
		unlockAchievement(achievement);
	}
}

customquote = quotes[Math.floor((Math.random() * quotes.length))]
campaignTrail_temp.running_mate_last_name = '';
campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/nW9C36y.png';
campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/krpw10L.png';
campaignTrail_temp.candidate_last_name = '';

campaignTrail_temp.cyoa = true;
e=campaignTrail_temp
// vars
var reveal = 0;
var bidenReplaced = 0
var dementiaMoments = 0;
var globalTension = 0;
var finalDelusion = 0;
e.noCounter = 0

const targetNode = document.getElementById("game_window");
const config = { attributes: true, childList: true, subtree: true };

function onGameWindowChanged(mutationList, observer) {
	updateQuestionBanner();
}

const observer = new MutationObserver(onGameWindowChanged);
observer.observe(targetNode, config);

function updateQuestionBanner() {
	let questionBanner = document.getElementById("campaign_sign");
	if(!questionBanner || questionBanner.nodeName != "DIV") return;
	let img = document.createElement('img');
	if(reveal == 0){
        img.src = 'https://i.imgur.com/jXrJXI3.png';
    }
    else if(reveal == 1 && bidenReplaced == 0){
        img.src = 'https://i.imgur.com/jeu8Ey5.png';
    }
    else{
        img.src = 'https://i.imgur.com/QMW4Y6J.png';
    }
	questionBanner.replaceWith(img);
}

function tunnel(new_q){
    return e.questions_json[e.questions_json.map(f=>f.pk).indexOf(new_q)];
}  

function modernizeStates() {
    // AL
    var id = e.states_json.map(p=>p.pk).indexOf(181)
    e.states_json[id].fields.electoral_votes = 9;
    e.states_json[id].fields.popular_votes = 359530;
        // Polarized
    var id = e.state_issue_score_json.map(p=>p.pk).indexOf(868)
    e.state_issue_score_json[id].fields.state_issue_score = -0.5;
    e.state_issue_score_json[id].fields.weight = 1.5;
    var id = e.state_issue_score_json.map(p=>p.pk).indexOf(869)
    e.state_issue_score_json[id].fields.state_issue_score = -0.5;
    e.state_issue_score_json[id].fields.weight = 1.5;
    var id = e.state_issue_score_json.map(p=>p.pk).indexOf(870)
    e.state_issue_score_json[id].fields.state_issue_score = -0.5;
    e.state_issue_score_json[id].fields.weight = 1.5;
    var id = e.state_issue_score_json.map(p=>p.pk).indexOf(871)
    e.state_issue_score_json[id].fields.state_issue_score = -0.5;
    e.state_issue_score_json[id].fields.weight = 1.5;
    var id = e.state_issue_score_json.map(p=>p.pk).indexOf(872)
    e.state_issue_score_json[id].fields.state_issue_score = -0.5;
    e.state_issue_score_json[id].fields.weight = 1.5;
    // AK
